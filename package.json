    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "config": "^3.3.9",
